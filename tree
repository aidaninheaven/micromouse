class Node:
    def __init__(self, data):
        self.left = None
        self.center = None
        self.right = None
        self.data = data

    def insert(self, data):
        if data < self.data:
            if self.left is None:
                self.left = Node(data)
            else:
                self.left.insert(data)
        elif data > self.data:
            if self.right is None:
                self.right = Node(data)
            else:
                self.right.insert(data)
        else:
            if self.center is None:
                self.center = Node(data)
            else:
                self.center.insert(data)

    def print_tree_structure(self):
        print(f"Node: {self.data}")
        
        print("Center:")
        if self.center:
            self.center.print_tree_structure()
        else:
            print("None")

        print("Left:")
        if self.left:
            self.left.print_tree_structure()
        else:
            print("None")

        print("Right:")
        if self.right:
            self.right.print_tree_structure()
        else:
            print("None")

        print()  # ‚Üê Whitespace only here, after Right


root = Node(0)
root.insert(1)

root.print_tree_structure()
